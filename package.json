{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node, express and mongoDB",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "start:script": "nodemon script.js",
    "start:prod": "cross-env PORT=3001 NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "lint": "eslint .",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js --no-source-maps || echo 'Warning: JS build failed but continuing'",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "check-deps": "node -e \"console.log('Skipping dependency check for compatibility')\"",
    "check-node": "node -e \"console.log('Node version:', process.version)\"",
    "build": "node -e \"console.log('ðŸ“‹ Build Environment:\\nNode version:', process.version, '\\nOS:', process.platform)\" && (npm run build:js || echo 'JS build skipped') && mkdir -p netlify/functions",
    "build:windows": "build.cmd",
    "build:linux": "bash ./build.sh",
    "dev": "netlify dev",
    "netlify:dev": "netlify dev",
    "extract-env": "node extract-env-vars.js"
  },
  "author": "Muhammad Saeed",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-xss-sanitizer": "^2.0.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.0",
    "morgan": "^1.9.1",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.0",
    "pug": "^3.0.3",
    "serverless-http": "^3.2.0",
    "sharp": "^0.34.3",
    "stripe": "^18.5.0",
    "validator": "^13.12.0",
    "xss": "^1.0.15",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "ndb": "^1.1.5",
    "netlify-cli": "^17.38.1",
    "parcel-bundler": "^1.12.5",
    "prettier": "^1.17.0"
  },
  "engines": {
    "node": "18.x"
  }
}
